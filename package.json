{
  "name": "squeeze-machine",
  "version": "1.0.0",
  "description": "Url Shortner using Node.js, Typescript, express. Tested with JEST",
  "main": "index.ts",
  "scripts": {
    "start": "node -r ts-node/register ./src/index.ts start -c ./configs/config.json",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "test": "jest --verbose --detectOpenHandles  -- allTests.test.ts",
    "docs": "typedoc --options typedoc.json",
    "make": "npm run build && npm run lint && npm run test && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N7fenrir/sqzm.git"
  },
  "keywords": [
    "link",
    "shortner",
    "TDD",
    "Backend",
    "TIER",
    "Node.js"
  ],
  "author": "Vighnesh Bheed",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/N7fenrir/sqzm/issues"
  },
  "homepage": "https://github.com/N7fenrir/sqzm#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/sqlite3": "^3.1.7",
    "@types/supertest": "^2.0.11",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^33.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "execa": "^5.1.1",
    "jest": "^27.1.1",
    "prettier": "^2.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.21.0",
    "typedoc-plugin-external-module-map": "^1.2.1",
    "typedoc-plugin-markdown": "^3.10.0",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "ajv": "^8.6.2",
    "body-parser": "^1.19.0",
    "commmander": "0.0.1-security",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "nanoid": "^3.1.25",
    "sqlite3": "^5.0.2",
    "valid-url": "^1.0.9"
